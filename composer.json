{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/composer": "^2.3",
        "composer/pcre": "^3.0",
        "erusev/parsedown": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "symfony/console": "^7",
        "symfony/dotenv": "^7",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7",
        "symfony/http-client": "^7",
        "symfony/mime": "^7",
        "symfony/monolog-bundle": "^3.7",
        "symfony/runtime": "^7",
        "symfony/twig-bundle": "^7",
        "symfony/yaml": "^7",
        "twig/extra-bundle": "^3",
        "twig/twig": "^3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4"
    },
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": {
            "composer/composer": "source",
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.3.3"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "phpstan"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        },
        "public-dir": "web"
    }
}
